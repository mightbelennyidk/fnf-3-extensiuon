{
  "manifest_version": 3,
  "name": "FNF Game Extension",
  "version": "1.0",
  "description": "Play FNF games directly from the extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.ico",
      "48": "icon48.ico",
      "128": "icon128.ico"
    }
  },
  "permissions": ["activeTab"]
}
